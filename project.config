[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/shanavash (shajanani)
[access "refs/for/refs/*"]
	read = group user/shanavash (shajanani)
	push = group user/shanavash (shajanani)
	label-Code-Review = -2..+2 group user/shanavash (shajanani)
	label-Verified = -1..+1 group user/shanavash (shajanani)
	rebase = group user/shanavash (shajanani)
	removeReviewer = group user/shanavash (shajanani)
	submit = group user/shanavash (shajanani)
[access "refs/heads/*"]
	read = group user/shanavash (shajanani)
	create = group user/shanavash (shajanani)
	push = +force group user/shanavash (shajanani)
	pushMerge = group user/shanavash (shajanani)
[access "refs/tags/*"]
	push = group user/shanavash (shajanani)
	push = +force group user/shanavash (shajanani)
